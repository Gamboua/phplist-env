paths:
    migrations: %%PHINX_CONFIG_DIR%%/public_html/lists/admin/plugins/phplistCaixaFunctionality/includes/src/Infrastructure/DB/Migrations
    seeds: %%PHINX_CONFIG_DIR%%/public_html/lists/admin/plugins/phplistCaixaFunctionality/includes/tests/Infrastructure/DB/Seeds

environments:
    default_migration_table: phinxlog
    default_database: development
    local:
        adapter: mysql
        host: {DATABASE_HOST}
        name: {DATABASE_NAME}
        user: {DATABASE_USER}
        pass: {DATABASE_PASSWORD}
        port: 3306
        charset: utf8

